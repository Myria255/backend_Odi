/* components/navigation-bottom/navigation-bottom.wxss */
.navigation{
  background-color: #ff7700;
  width: 100%;
  display: flex;
  height: 100rpx;
  justify-content: space-around;
  align-items: center;
}
.icon{
  display: flex;
  display: block;
  text-align: center;
}
.icon image{
  width: 15px;
  height: 15px;
}
.text{
  font-size: 12px;
  color: #FFFFFF;
  font-family: 'Times New Roman', Times, serif;
}
